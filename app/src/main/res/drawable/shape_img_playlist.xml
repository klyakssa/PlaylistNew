<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:gravity="center">
        <shape android:shape="rectangle">
            <stroke
                android:color="@color/YP_text_gray"
                android:width="1dp"
                android:dashWidth="24dp"
                android:dashGap="32dp"/>
            <corners android:radius="8dp"/>
        </shape>
    </item>
    <item
        android:drawable="@drawable/add_photo"
        android:bottom="20dp"
        android:left="20dp"
        android:right="20dp"
        android:top="20dp"
        android:gravity="center">
    </item>

    <item>
        <shape android:shape="rectangle">
            <corners android:radius="8dp"/>
        </shape>
    </item>

</layer-list>